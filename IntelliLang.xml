<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="JPAQL" injector-id="java">
      <display-name>Query.value (org.springframework.data.jpa.repository)</display-name>
      <place><![CDATA[psiMethod().withName("value").withParameters().definedInClass("org.springframework.data.jpa.repository.Query")]]></place>
    </injection>
    <injection language="Angular2HTML" injector-id="xml">
      <display-name>body</display-name>
      <place><![CDATA[xmlTag().withLocalName(string().equalTo("body")).withNamespace(string().equalTo("http://www.w3.org/1999/html"))]]></place>
    </injection>
  </component>
</application>