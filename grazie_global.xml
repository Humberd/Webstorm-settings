<application>
  <component name="GraziConfig">
    <option name="enabledLanguages">
      <set>
        <option value="English (US)" />
        <option value="Polish" />
      </set>
    </option>
    <option name="enabledGrammarStrategies">
      <set>
        <option value="JavaScript:JavaScript" />
        <option value="nl.rubensten.texifyidea:Latex" />
        <option value="org.asciidoctor.intellij.asciidoc:AsciiDoc" />
        <option value="tanvd.grazi:JSON" />
        <option value="tanvd.grazi:TEXT" />
      </set>
    </option>
    <option name="userDisabledRules">
      <set>
        <option value="PASSIVE_VOICE" />
      </set>
    </option>
    <option name="userEnabledRules">
      <set>
        <option value="UPPERCASE_SENTENCE_START" />
      </set>
    </option>
    <option name="suppressingContext">
      <SuppressingContext>
        <option name="suppressed">
          <set>
            <option value="callbacks are configured" />
            <option value="config is consumed" />
          </set>
        </option>
      </SuppressingContext>
    </option>
    <option name="checkingContext">
      <CheckingContext>
        <option name="isCheckInStringLiteralsEnabled" value="true" />
      </CheckingContext>
    </option>
  </component>
</application>